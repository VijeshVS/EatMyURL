generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PG_DATABASE_URL")
}

model user {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  password   String
  name       String?
  imageurl   String?
  created_at DateTime
  links      links[]
}

model links {
  id         Int      @id @default(autoincrement())
  user       user     @relation(fields: [user_id], references: [id])
  user_id    Int
  short_code String   @unique
  long_url   String
  created_at DateTime
}
